{
    "name": "natewarr-play",
    "version": "1.0.0",
    "author": "Nate Warren",
    "license": "MIT",
    "summary": "puppet-play is a puppet component module to deploy play-framework applications on Linux",
    "source": "https://github.com/natewarr/puppet-play.git",
    "project_page": "https://github.com/natewarr/puppet-play",
    "issues_url": "https://github.com/natewarr/puppet-play/issues",
    "tags": [
        "play",
        "framework",
        "deploy"
    ],
    "operatingsystem_support": [
        {
            "operatingsystem": "RedHat",
            "operatingsystemrelease": [
                "6.0"
            ]
        },
        {
            "operatingsystem": "Ubuntu",
            "operatingsystemrelease": [
                "12.04",
                "14.04"
            ]
        }
    ],
    "dependencies": [
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">=4.1.0 <5.0.0"
        },
        {
            "name": "maestrodev/wget",
            "version_requirement": ">= 1.5.6"
        }
    ]
}
